{
  "name": "github-to-omnifocus",
  "version": "0.0.0",
  "description": "Sync GitHub issues to OmniFocus",
  "author": "Scott Hardy",
  "license": "MIT",
  "repository": "git@github.com:scott113341/github-to-omnifocus.git",
  "homepage": "https://github.com/scott113341/github-to-omnifocus",
  "bugs": "https://github.com/scott113341/github-to-omnifocus/issues",
  "keywords": [],
  "main": "lib/index.js",
  "files": [
    "lib/",
    "src/"
  ],
  "bin": {
    "github-to-omnifocus": "lib/index.js"
  },
  "scripts": {
    "build": "shx rm -rf lib/ && shx mkdir lib/ && babel --copy-files --source-maps --out-dir lib/ src/",
    "lint": "semistandard --fix",
    "postinstall": "postinstall-build lib/ \"npm run build\"",
    "test": "npm run build && node lib/test/index.js"
  },
  "dependencies": {
    "postinstall-build": "2.1.3"
  },
  "devDependencies": {
    "babel-cli": "6.23.0",
    "babel-plugin-transform-async-to-generator": "6.22.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-stage-0": "6.22.0",
    "np": "2.12.0",
    "semistandard": "9.2.1",
    "shx": "0.2.2",
    "source-map-support": "0.4.11",
    "tape": "4.6.3"
  },
  "semistandard": {
    "ignore": [
      "/lib/"
    ]
  }
}
